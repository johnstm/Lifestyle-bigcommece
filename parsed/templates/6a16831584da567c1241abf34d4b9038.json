{
  "components/carousel-content": "<div data-animation-in=\"fadeIn\" class=\"animated fadeIn heroCarousel-content{{#unless show_background}} heroCarousel-content--empty{{/unless}}\">\r\n    <h1 data-animation-in=\"slideInUp\" class=\"animated slideInUp heroCarousel-title\">{{{heading}}}</h1>\r\n    <p data-animation-in=\"slideInUp\" class=\"animated slideInUp  heroCarousel-description\">{{{text}}}</p>\r\n    {{#if button_text}}\r\n        <span data-animation-in=\"slideInUp\" class=\"animated slideInUp heroCarousel-action button button--primary button--large\">\r\n          <span class=\"heroCarousel-action-inn\">\r\n            {{button_text}}<svg ><use xlink:href=\"#icon-arrow-slider\"></use></svg>\r\n          </span>\r\n        </span>\r\n    {{/if}}\r\n</div>\r\n"
}
